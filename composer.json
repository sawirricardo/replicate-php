{
    "name": "sawirricardo/replicate-php",
    "description": "PHP client for the Replicate API",
    "keywords": [
        "sawirricardo",
        "replicate-php",
        "replicate",
        "machine",
        "learning",
        "AI",
        "artificial",
        "intelligence"
    ],
    "homepage": "https://github.com/sawirricardo/replicate-php",
    "license": "MIT",
    "authors": [
        {
            "name": "sawirricardo",
            "email": "sawir.ricardo@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "sammyjo20/saloon": "^2.5"
    },
    "require-dev": {
        "pestphp/pest": "^1.20",
        "laravel/pint": "^1.2",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Sawirricardo\\Replicate\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sawirricardo\\Replicate\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
